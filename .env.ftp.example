FTP_HOST=ftp.example.com
FTP_PORT=21
FTP_USER=your_ftp_username
FTP_PASSWORD=your_ftp_password
FTP_SECURE=false
# Path to your site root, commonly /public_html or /domains/example.com/public_html
FTP_REMOTE_DIR=/public_html
# Set to true to clear remote dir before upload (use with caution)
FTP_CLEAN=false